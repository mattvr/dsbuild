{
  "name": "@orgsoft/dsbuild",
  "version": "0.2.11",
  "exports": "./mod.ts",
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "dom.asynciterable", "esnext", "deno.ns"],
    "jsx": "react-jsxdev",
    "jsxImportSource": "react"
  },
  "vendor": false,
  "tasks": {
    "install": "deno install -frAg jsr:@orgsoft/dsbuild -n dsbuild",
    "install:dev": "deno install -frAg --name dsbuild mod.ts --config=deno.json"
  },
  "imports": {
    "react": "npm:react@^18.3.1",
    "react-dom/client": "npm:react-dom@18.3.1/client",
    "react-dom": "npm:react-dom@18.3.1"
  }
}
